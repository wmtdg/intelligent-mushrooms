	accactiveNavigationSection  = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
}

var tab = $(".myaccordionclass > div[data-id='" + accactiveNavigationSection+ "']");
$( ".myaccordionclass" ).accordion({ active: tab.index(".liboxes") });

$('.libox ul li a').click(function(){
				  $('.liboxselectedanchor').removeClass('liboxselectedanchor');
				  $(this).addClass('liboxselectedanchor');
				  });

$("#e1").select2();
$("#e1").change(function(){
	var redirectPage = $("#e1").val();
	window.location = redirectPage;
});
$('#slideshow').fadeSlideShow();

function divToggle (targetCodeDiv, targetToggleDiv)
{
	var toggleText = $("#" + targetToggleDiv).attr("altvalue");
	var currText = $("#" + targetToggleDiv).text();
	$("#" + targetToggleDiv).text(toggleText);
	$("#" + targetToggleDiv).attr("altvalue", currText);
	if ($("#" + targetCodeDiv).hasClass("hide"))
		$("#" + targetCodeDiv).removeClass("hide");
	else
		$("#" + targetCodeDiv).addClass("hide");
}

jQuery.fx.speeds = {
	slow: 0,
	fast: 0,
	// Default speed
	_default: 0
};
});
</script>

